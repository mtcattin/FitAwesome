<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_exercise_log"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.matthew.fitawesome.ExerciseLog">

    <Button
        android:text="Week 5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button5" />

    <Button
        android:text="Week 6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_week1"
        android:layout_toEndOf="@+id/button5"
        android:id="@+id/button6" />

    <Button
        android:text="Week 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btn_week1"
        android:layout_alignEnd="@+id/button6"
        android:id="@+id/viewWout_btn" />

    <Button
        android:text="Week 3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/viewWout_btn"
        android:layout_toEndOf="@+id/viewWout_btn"
        android:id="@+id/button3" />

    <Button
        android:text="Week 4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button3"
        android:layout_alignParentEnd="true"
        android:id="@+id/button4" />

    <Button
        android:text="Week 7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button6"
        android:layout_toStartOf="@+id/button4"
        android:id="@+id/button7" />

    <Button
        android:text="Week 8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button7"
        android:layout_toEndOf="@+id/button7"
        android:id="@+id/button8" />

    <Button
        android:text="Week 9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp"
        android:id="@+id/button9" />

    <Button
        android:text="Week 10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button9"
        android:layout_toEndOf="@+id/button9"
        android:id="@+id/button10" />

    <Button
        android:text="Week 11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button10"
        android:layout_toEndOf="@+id/button10"
        android:id="@+id/button11" />

    <Button
        android:text="Week 12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button11"
        android:layout_toEndOf="@+id/button11"
        android:id="@+id/button12" />

    <Button
        android:text="Week 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="17dp"
        android:id="@+id/btn_week1"
        android:layout_above="@+id/button5"
        android:layout_alignParentStart="true" />
</RelativeLayout>
